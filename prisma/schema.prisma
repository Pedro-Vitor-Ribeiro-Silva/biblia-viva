generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Devocional {
  id        String   @id @default(uuid())
  date      DateTime @unique
  content   String
  createdAt DateTime @default(now())
}